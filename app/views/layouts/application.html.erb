<!DOCTYPE html>
<html>
<head>
  <title>EnbakeCompetition</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if user_signed_in? %>
	<li>
	  <%= link_to 'Sign Out', destroy_user_session_path, :method => :delete, :class => "text-align:right" %>
	</li>
<% else %>
	<li>
		<%= link_to 'Sign Up', new_user_registration_path %>
	</li>
<% end %>
<%= yield %>

</body>
</html>
